- if like.blank?
  = button_to prototype_likes_path(prototype.id), class: 'btn btn-default', remote: true do
    = image_tag("icon_heart.svg")
    Like
    %span.num
      = prototype.likes.count
- else
  = button_to prototype_like_path(prototype.id, like.id), class: 'btn btn-default', method: :delete, remote: true do
    = image_tag("icon_heart_red.svg")
    Like
    %span.num
      = prototype.likes.count